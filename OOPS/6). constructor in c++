#include <iostream>
using namespace std;

class MyClass {
    int a;
    double b;

public:
    MyClass(int x, double y) : a(x), b(y) {
        cout << "Constructor called: a = " << a << ", b = " << b << endl;
    }
};

int main() {
    MyClass obj(10, 5.5); // Creating an object with parameters
    return 0;
}
