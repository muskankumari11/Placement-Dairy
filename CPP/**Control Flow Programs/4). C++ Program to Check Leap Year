#include<iostream>
using namespace std;
bool checkYear(int year){
    if(year%400==0){
        return true;
    }
    else if(year%4==0){
        return true;
    }
    else if(year%100==0){
        return false;
    }
    else{
        return false;
    }
}
int main(){
    int year=2000;
    checkYear(year)? cout<<"leap year":
    cout<<"not a leap year";
    
    return 0;
}
