#include<iostream>
using namespace std;
void check(char ch){
    if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u'||
    ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U'){
        cout<<ch<<" "<<"is vowel";
    }
    else{
        cout<<ch<<" "<<"is consonant";
    }
}
int main(){
    check('E');
    return 0;
}
