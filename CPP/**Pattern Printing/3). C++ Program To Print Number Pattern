#include<iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter number: ";
    cin >> n;

    int k = 1;
    for (int i = 0; i <= n; i++) {         // Outer loop
        for (int j = 0; j <= i; j++) {     // Inner loop (corrected to j++)
            cout << k <<" ";
            k++;
        }
        cout << endl;  
    }
    return 0;
}
