#include <bits/stdc++.h>
using namespace std;

void maxminelement(const vector<int>& arr) {
    if (arr.empty()) {
        cout << "Array is empty." << endl;
        return;
    }

    int minVal = arr[0];
    int maxVal = arr[0];

    for (int i = 1; i < arr.size(); i++) {
        if (arr[i] < minVal) {
            minVal = arr[i];
        }
        if (arr[i] > maxVal) {
            maxVal = arr[i];
        }
    }

    cout << "Min element is: " << minVal << endl;
    cout << "Max element is: " << maxVal << endl;
}

int main() {
    vector<int> arr = {4, 2, 5, 6};
    maxminelement(arr);
    return 0;
}

