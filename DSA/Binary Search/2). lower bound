#include <iostream>
using namespace std;

int main() {
    int arr[9] = {1, 2, 4, 5, 9, 15, 18, 21, 24};
    int n = 9;
    int x = 20;
    
    for (int i = 0; i < n; i++) {
        if (arr[i] > x) {
            if (i == 0) {
                cout << "No smaller element exists";  
            } else {
                cout << arr[i - 1];  
            }
            break;
        }
    }

    return 0;
}
