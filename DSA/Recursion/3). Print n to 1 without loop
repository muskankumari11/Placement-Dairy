class Solution {
  public:
    void printNos(int N) {
        // code here
        if(N==0)
        return;
        cout<<N<<" ";
        printNos(N-1);
        // cout<<N<<" ";
    }
};

//full code
#include <iostream>
using namespace std;

class Solution {
  public:
    void printNos(int N) {
        if (N == 0) return;
        cout << N << " ";
        printNos(N - 1);
    }
};

int main() {
    Solution obj;
    int n;
    cout << "Enter a number: ";
    cin >> n;
    obj.printNos(n);
    cout << endl;
    return 0;
}
